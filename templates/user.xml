<templateSet group="user">
  <template name="linf" value="\Log::info($END$);" description="laravel log info" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="lerr" value="\Log::error($END$);" description="laravel log error" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="imp" value="import $END$ from '$library$'" description="Import statement - import {b} from 'a'" toReformat="false" toShortenFQNames="true">
    <variable name="library" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>